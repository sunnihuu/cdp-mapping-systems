{
  "version": 8,
  "name": "Protomaps Basic",
  "sources": {
    "protomaps": {
      "type": "vector",
      "tiles": [
        "https://api.protomaps.com/tiles/v4/{z}/{x}/{y}.mvt?key=9becfca38f3f900c"
      ],
      "minzoom": 0,
      "maxzoom": 14
    }
  },
  "glyphs": "https://protomaps.github.io/basemaps-assets/fonts/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {"background-color": "#ddeeff"}
    },
    {
      "id": "water",
      "type": "fill",
      "source": "protomaps",
      "source-layer": "water",
      "paint": {"fill-color": "#a0c8f0"}
    },
    {
      "id": "landuse",
      "type": "fill",
      "source": "protomaps",
      "source-layer": "landuse",
      "paint": {"fill-color": "#e0e0d8"}
    },
    {
      "id": "roads",
      "type": "line",
      "source": "protomaps",
      "source-layer": "roads",
      "paint": {"line-color": "#bbb", "line-width": 1}
    },
    {
      "id": "buildings",
      "type": "fill",
      "source": "protomaps",
      "source-layer": "buildings",
      "paint": {"fill-color": "#d1cfcf"}
    }
  ],
  "id": "protomaps-basic"
}
